    "@babel/core": "^7.0.0-beta.39",
    "@babel/template": "^7.0.0-beta.39",
    "@babel/types": "^7.0.0-beta.39"
  },
  "dependencies": {
    "@babel/register": "^7.0.0-beta.39"
  },
  "prettier": {
    "singleQuote": false
  },
  "lint-staged": {
    "*.@(js|jsx)": [
      "npm run prettier-write --",
      "git add"
    ]
  }
}
